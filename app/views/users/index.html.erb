<div class="container">
	<div class="row">

    <div class="actions">
        <%= link_to 'New User', new_user_path, :class => ["btn","primary"] %>
    </div>

    <table id="user-table" class="zebra-striped">
      <tr>
        <th>Email</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>

    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %>  <%= user.name %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', feedback, :confirm => 'Are you sure?' %></td>
      </tr>
    <% end %>
    </table>
  </div>
</div>

